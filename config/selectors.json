{
  "townwork": {
    "name": "タウンワーク",
    "base_url": "https://townwork.net",
    "search_url_pattern": "https://townwork.net/prefectures/{area}/job_search/?keyword={keyword}&page={page}",
    "sort": {
      "param": "sort",
      "newest": "1"
    },
    "selectors": {
      "job_cards": "[class*='jobCard']",
      "title": "[class*='title__']",
      "company": "[class*='employerName']",
      "location": "[class*='accessText']",
      "salary": "[class*='salaryText']",
      "employment_type": "[class*='jobType']",
      "detail_link": "[class*='jobCard']"
    },
    "detail_selectors": {
      "company_name": "[class*='companyName'], [class*='employerName']",
      "postal_code_address": "body",
      "phone": "body",
      "job_number": "body",
      "business_content": "body"
    },
    "area_codes": {
      "北海道": "hokkaido",
      "青森": "aomori",
      "岩手": "iwate",
      "宮城": "miyagi",
      "秋田": "akita",
      "山形": "yamagata",
      "福島": "fukushima",
      "茨城": "ibaraki",
      "栃木": "tochigi",
      "群馬": "gunma",
      "埼玉": "saitama",
      "千葉": "chiba",
      "東京": "tokyo",
      "神奈川": "kanagawa",
      "新潟": "niigata",
      "富山": "toyama",
      "石川": "ishikawa",
      "福井": "fukui",
      "山梨": "yamanashi",
      "長野": "nagano",
      "岐阜": "gifu",
      "静岡": "shizuoka",
      "愛知": "aichi",
      "三重": "mie",
      "滋賀": "shiga",
      "京都": "kyoto",
      "大阪": "osaka",
      "兵庫": "hyogo",
      "奈良": "nara",
      "和歌山": "wakayama",
      "鳥取": "tottori",
      "島根": "shimane",
      "岡山": "okayama",
      "広島": "hiroshima",
      "山口": "yamaguchi",
      "徳島": "tokushima",
      "香川": "kagawa",
      "愛媛": "ehime",
      "高知": "kochi",
      "福岡": "fukuoka",
      "佐賀": "saga",
      "長崎": "nagasaki",
      "熊本": "kumamoto",
      "大分": "oita",
      "宮崎": "miyazaki",
      "鹿児島": "kagoshima",
      "沖縄": "okinawa"
    },
    "pagination": {
      "type": "page_number",
      "param": "page",
      "start": 1
    },
    "filters": {
      "query_params": {
        "employment_type": "employment",
        "salary_min": "salary_min",
        "shift": "shift"
      },
      "options": {
        "employment_type": {
          "アルバイト": "baito",
          "パート": "part",
          "正社員": "full"
        },
        "shift": {
          "日勤": "day",
          "夜勤": "night"
        }
      }
    }
  },
  "baitoru": {
    "name": "バイトル",
    "base_url": "https://www.baitoru.com",
    "search_url_pattern": "https://www.baitoru.com/search/?kw={keyword}&pref={area}&p={page}",
    "selectors": {
      "job_cards": ".cassetteRecruit",
      "title": ".cassetteRecruit__name",
      "company": ".cassetteRecruit__name em",
      "location": ".cassetteRecruit__area",
      "salary": ".cassetteRecruit__salary",
      "employment_type": ".cassetteRecruit__attribute",
      "detail_link": "a.cassetteRecruit__name",
      "phone": ".tel-number",
      "address": ".address-text"
    },
    "detail_selectors": {
      "company_kana": ".company-name-kana",
      "postal_code": ".zip-code",
      "fax": ".fax",
      "job_number": ".job-code",
      "business_content": ".business-info"
    },
    "pagination": {
      "type": "page_number",
      "param": "p",
      "start": 1
    }
  },
  "indeed": {
    "name": "Indeed",
    "base_url": "https://jp.indeed.com",
    "search_url_pattern": "https://jp.indeed.com/jobs?q={keyword}&l={area}&start={offset}",
    "selectors": {
      "job_cards": ".job_seen_beacon",
      "title": ".jobTitle span",
      "company": ".companyName",
      "location": ".companyLocation",
      "salary": ".salary-snippet",
      "employment_type": ".metadata",
      "detail_link": "a.jcs-JobTitle",
      "job_description": ".job-snippet"
    },
    "detail_selectors": {
      "phone": ".icl-u-lg-mr--sm",
      "full_description": "#jobDescriptionText",
      "job_number": ".jobsearch-JobMetadataHeader-item"
    },
    "pagination": {
      "type": "offset",
      "param": "start",
      "start": 0,
      "increment": 10
    }
  },
