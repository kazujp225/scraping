{
  "townwork": {
    "name": "タウンワーク",
    "base_url": "https://townwork.net",
    "search_url_pattern": "https://townwork.net/{area}/search/?keyword={keyword}&page={page}",
    "selectors": {
      "job_cards": ".jbc-l-main-list__item",
      "title": ".jbc-c-heading-joblist",
      "company": ".jbc-c-heading-joblist__catch",
      "location": ".jbc-c-txt-access",
      "salary": ".jbc-c-txt-salary",
      "employment_type": ".jbc-c-label-employment",
      "detail_link": "a.jbc-c-btn-detail",
      "phone": ".jbc-c-txt-tel",
      "address": ".jbc-c-txt-address",
      "job_description": ".jbc-c-txt-description"
    },
    "detail_selectors": {
      "company_kana": ".company-kana",
      "postal_code": ".postal-code",
      "fax": ".fax-number",
      "recruiter": ".recruiter-name",
      "recruiter_email": ".recruiter-email",
      "job_number": ".job-id",
      "business_content": ".business-description"
    },
    "pagination": {
      "type": "page_number",
      "param": "page",
      "start": 1
    }
    ,
    "filters": {
      "query_params": {
        "employment_type": "employment",
        "salary_min": "salary_min",
        "shift": "shift"
      },
      "options": {
        "employment_type": {
          "アルバイト": "baito",
          "パート": "part",
          "正社員": "full"
        },
        "shift": {
          "日勤": "day",
          "夜勤": "night"
        }
      }
    }
  },
